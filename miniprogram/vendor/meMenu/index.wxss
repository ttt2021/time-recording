.contant-column {
  height: 100rpx;
  /* padding-top: 30rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #F8F8FF;
  /* border: 1px solid #000; */
  margin: 0 40rpx;
}

.contant-icon {
  display: flex;
  justify-content: center;
  align-items: center;
}

.contant-title {
  margin-left: 10rpx;
  font-size: 30rpx;
}

.img, .arrow {
  height: 50rpx;
  width: 50rpx;
}

.btn {
  padding: 0;
  margin: 0 40rpx;
  outline: 0;
  border: 0;
  /* border: 1px solid #000; */
  font-size: 30rpx;
}

.btn-arrow {
  width: 28%;
  padding-top: 20rpx;
  /* margin-left: -50rpx; */
}

.consultation .btn {
  /* background-color: #A5D63F; */
  width: 100%;
  border: 0;
  background-color: transparent;
  outline: none;
}